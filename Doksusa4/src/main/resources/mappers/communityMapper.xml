<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doksusa.cm">

	<select id="cm_selectAll" resultType="cmdto">
		select * from community</select>
	<select id="cm_select" resultType="cmdto" parameterType="int">
		select * from community where c_num=#{c_num}</select>
	<insert id="cm_insert" parameterType="cmdto">
		insert into community values (c_num_SEQ.nextval,#{c_group},sysdate,#{title},#{f_foreword},#{u_num})</insert>
	<update id="cm_update" parameterType="int">
		update community,content set title=#{title},c_content=#{c_content} where c_num=#{c_num} and c_group=#{c_group}</update>
	<delete id="cm_delete" parameterType="int">
		delete from community,content where c_num=#{c_num}</delete>

</mapper>